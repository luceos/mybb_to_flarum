{
    "name": "luceos/mybb-to-flarum",
    "description": "Migrate a MyBB Forum to a fresh Flarum instance",
    "type": "flarum-extension",
    "require": {
        "flarum/core": "^1.0",
        "flarum/discussion-seo-slug-redirect": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Michael V.",
            "email": "michael@michaelbelgium.me",
            "homepage": "http://michaelbelgium.me"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/MichaelBelgium"
        }
    ],
    "autoload": {
        "psr-4": {
            "Michaelbelgium\\Mybbtoflarum\\": "src/"
        }
    },
    "conflict": {
        "michaelbelgium/mybb-to-flarum": "*"
    },
    "extra": {
        "flarum-extension": {
            "title": "MyBB to Flarum",
            "icon": {
                "name": "fas fa-exchange-alt",
                "backgroundColor": "#FFB300",
                "color": "#fff"
            }
        }
    }
}
